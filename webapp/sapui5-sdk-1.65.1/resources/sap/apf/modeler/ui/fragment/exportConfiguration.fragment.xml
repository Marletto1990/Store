<core:FragmentDefinition xmlns="sap.m"
	xmlns:core="sap.ui.core" xmlns:f="sap.ui.layout.form">
	<Dialog xmlns="sap.m" id="idExportConfigDialog" icon="" class="sapUiSizeCompact dialogContentPadding"
		showHeader="true" type="Standard" initialFocus="idExportConfigDialog">
		<VBox>
			<HBox>
				<Link xmlns="sap.m" id="idDownloadConfig" press="_handleOpenConfigLinkPress" />
			</HBox>
			<HBox>
				<Link xmlns="sap.m" id="idDownloadTextProperty" press="_handleOpenTextsLinkPress" />
			</HBox>
		</VBox>
		<beginButton>
			<Button xmlns="sap.m" id="idCloseButton" type="Default" width=""
				enabled="true" visible="true" press="_handleCloseOfExportDialog">
			</Button>
		</beginButton>
	</Dialog>
</core:FragmentDefinition>