<core:FragmentDefinition xmlns="sap.m"
	xmlns:core="sap.ui.core">
	<Dialog xmlns="sap.m" id="idDialogWithTwoButtons" type="Message"
		class="sapUiSizeCompact dialogContentPadding" initialFocus="idDialogWithTwoButtons" afterClose="handleCancel">
		<content>
			<Label xmlns="sap.m" id="idLabelForDialog" class="dialogText" />
		</content>
		<beginButton>
			<Button xmlns="sap.m" id="idBeginButtonForDialog"
				press="handleBeginButtonDialogWithTwoButtons" />
		</beginButton>
		<endButton>
			<Button xmlns="sap.m" id="idEndButtonForDialog" press="handleCancel" />
		</endButton>
	</Dialog>
</core:FragmentDefinition>