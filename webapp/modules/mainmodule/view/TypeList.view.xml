<mvc:View
	xmlns:mvc="sap.ui.core.mvc"
	xmlns:custom="http://schemas.sap.com/sapui5/extension/sap.ui.core.CustomData/1"
	xmlns:f="sap.ui.layout.form" 
	xmlns:l="sap.ui.layout"
	controllerName="app/modules/mainmodule/controller/TypeList"
	xmlns="sap.m">
			<Page 
				id="typePage" 
				title="Уточните категорию" 
				icon="sap-icon://action" 
				class="sapUiStdPage" 
				showNavButton="true"
				navButtonPress="onTypesNavBack">
				<content>
					<List items = "{myModel>/Categories/0/type}" id="typeList">
						<items>
							<StandardListItem 
								title="{myModel>title}" 
								type="Navigation" 
								press="onTypeListItemPress"/>
						</items>
					</List>
				</content>
				<footer>
					<OverflowToolbar>
						<Button icon="sap-icon://home" width="20%" press="goToStart"/>
						<Button icon="sap-icon://cart" width="20%" type="Accept"/>
						<Button icon="sap-icon://customize" width="20%"/>
						<Button icon="sap-icon://customer" width="20%"/>
					</OverflowToolbar>
				</footer>
			</Page>
</mvc:View>