<mvc:View
	xmlns:mvc="sap.ui.core.mvc"
	xmlns:custom="http://schemas.sap.com/sapui5/extension/sap.ui.core.CustomData/1"
	xmlns:f="sap.ui.layout.form" 
	xmlns:l="sap.ui.layout"
	controllerName="mainpath.controller.Contacts"
	xmlns="sap.m">
	<Page 
		id="sContacts" 
		title="Контакты" 
		class="sapUiStdPage" 
		showNavButton="false"
		navButtonText="Back" 
		navButtonPress="onPressDetailBack">
		<content>
			<Breadcrumbs 	
			currentLocationText="{myModel>/Remote_current/title}" 
			class = "sapUiSmallMarginBegin sapUiSmallMarginTop"
			links = "{myModel>/Remote}" >
				<Link press="onBreadcrumbsPress" text="{myModel>title}"/>
			</Breadcrumbs>
			<Label text="Контакты"/>
		</content>
	</Page>
</mvc:View>