<mvc:View
	xmlns:mvc="sap.ui.core.mvc"
	xmlns:custom="http://schemas.sap.com/sapui5/extension/sap.ui.core.CustomData/1"
	xmlns:f="sap.ui.layout.form" 
	xmlns:l="sap.ui.layout"
	controllerName="mainpath.controller.Callback"
	xmlns="sap.m">
	<Page 
		id="sCallBack" 
		title="Форма обратной связи" 
		class="sapUiStdPage" 
		showNavButton="false"
		navButtonText="Back" 
		navButtonPress="onPressDetailBack">
		<content>
			<Breadcrumbs 	
				currentLocationText="{myModel>/Remote_current/title}" 
				class = "sapUiSmallMarginBegin sapUiSmallMarginTop"
				links = "{myModel>/Remote}">
				<Link press="onBreadcrumbsPress" text="{myModel>title}"/>
			</Breadcrumbs>
			
			<Label text="Как к Вам обращаться" />
			<Input/>
			<Label text="Телефон для связи" />
			<Input/>
			<Button text="Оставить заявку"/>
		</content>
	</Page>
</mvc:View>